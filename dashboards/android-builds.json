{
  "dashboard": {
    "id": null,
    "uid": "android-builds",
    "title": "Android Build Times & Test Pass Rates",
    "tags": [
      "android",
      "builds",
      "testing"
    ],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Build Duration Over Time",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "android_build_duration_seconds",
            "legendFormat": "{{build_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 2,
        "title": "Test Pass Rate",
        "type": "gauge",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "android_test_pass_total / android_test_total * 100",
            "legendFormat": "pass %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "max": 100,
            "thresholds": {
              "steps": [
                {
                  "value": 0,
                  "color": "red"
                },
                {
                  "value": 80,
                  "color": "yellow"
                },
                {
                  "value": 95,
                  "color": "green"
                }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Build Success/Failure",
        "type": "piechart",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 0
        },
        "targets": [
          {
            "expr": "android_build_total",
            "legendFormat": "{{result}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 4,
        "title": "Gradle Task Duration",
        "type": "table",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "topk(10, android_gradle_task_seconds)",
            "legendFormat": "{{task}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 5,
        "title": "APK Size Trend",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "expr": "android_apk_size_bytes",
            "legendFormat": "{{variant}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes"
          }
        }
      }
    ]
  }
}